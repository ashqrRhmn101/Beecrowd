#include<stdio.h>
#define PI 3.14159
int main()
{
     double A,B,C;
     double triangulo,circulo,treapezio,quadrado,retargulo;
     scanf("%lf %lf %lf",&A,&B,&C);
     triangulo= (A*C)/2;
     circulo= (PI*C*C);
     treapezio= ((A+B)*C)/2;
     quadrado= B*B;
     retargulo= A*B;
     
     printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n",triangulo,circulo,treapezio,quadrado,retargulo);
     return 0;
}
